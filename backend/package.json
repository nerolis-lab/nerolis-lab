{
  "name": "Sleep API",
  "version": "2.7.1",
  "private": true,
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json && tsc-alias && tsoa spec",
    "postbuild": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/**/*.json src/**/*.png src/assets/* src/assets/**/* dist/ && cp ../CHANGELOG.md dist/",
    "test": "vitest --run --coverage",
    "test-watch": "vitest --changed",
    "benchmark-set": "vitest run benchmark --mode benchmarking --update",
    "benchmark-test": "vitest run benchmark --mode benchmarking",
    "compile": "bun x tsc --noEmit",
    "start": "npm run build && node ./dist/app.js",
    "dev": "bun i && bun --watch src/app.ts",
    "type-watch": "bun x tsc --noEmit --watch"
  },
  "files": ["dist"],
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/cors": "^2.8.15",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.11.20",
    "@types/seedrandom": "^3.0.8",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^3.2.3",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "sqlite3": "^5.1.6",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vimic": "^1.2.4",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.40",
    "axios": "^1.10.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "discord-oauth2": "^2.12.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "google-auth-library": "^10.2.1",
    "knex": "^3.1.0",
    "morgan": "~1.10.1",
    "mysql2": "~3.14.3",
    "patreon-api.ts": "^0.14.2",
    "seedrandom": "^3.0.5",
    "sleepapi-common": "file:../common",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.6.0",
    "workerpool": "^9.3.3"
  },
  "bundleDependencies": ["sleepapi-common"]
}
