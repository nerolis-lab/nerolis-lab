{
  "name": "sleep-api",
  "version": "2.7.1",
  "private": true,
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json && tsc-alias && tsoa spec",
    "postbuild": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/**/*.json src/**/*.png src/assets/* src/assets/**/* dist/",
    "test": "vitest --run --coverage",
    "test-watch": "vitest --changed",
    "compile": "bun x tsc --noEmit",
    "start": "npm run build && node ./dist/app.js",
    "dev": "bun i && bun --watch src/app.ts",
    "type-watch": "bun x tsc --noEmit --watch"
  },
  "files": ["dist"],
  "devDependencies": {
    "@types/bun": "^1.1.17",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.11.20",
    "@types/seedrandom": "^3.0.8",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "@vitest/coverage-v8": "^3.1.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "sleepapi-common": "file:../common",
    "sqlite3": "^5.1.6",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.8.3",
    "vimic": "^1.2.4",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.33",
    "axios": "^1.8.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "discord-oauth2": "^2.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.10.0",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "mysql2": "~3.9.7",
    "patreon-api.ts": "^0.12.3",
    "seedrandom": "^3.0.5",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.6.0",
    "workerpool": "^9.2.0"
  },
  "bundleDependencies": ["sleepapi-common"]
}
