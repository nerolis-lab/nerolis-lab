{
  "name": "sleepapi-bot",
  "version": "1.0.0",
  "private": true,
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "prebuild": "npm run clean",
    "build": "vite build",
    "start": "node ./dist/app.js",
    "dev": "vite build && concurrently \"vite build --watch\" \"nodemon dist/app.js\" --kill-others --names \"vite,node\"",
    "test": "vitest --run --coverage",
    "test-watch": "vitest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "common": "file:../common",
    "discord.js": "^14.21.0",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/morgan": "^1.9.10",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3",
    "vimic": "^1.2.4",
    "vite": "^7.0.2",
    "vitest": "^3.2.4"
  }
}
