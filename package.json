{
  "name": "sleepapi",
  "version": "2.29.0",
  "private": true,
  "type": "module",
  "scripts": {
    "release": "semantic-release",
    "postinstall": "node .github/install-commit-hook.js",
    "lint": "npm run eslint-check && npm run prettier-check",
    "lint-fix": "npm run eslint-fix && npm run prettier-fix",
